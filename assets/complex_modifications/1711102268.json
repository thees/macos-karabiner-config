{
    "title": "Maps Eject to Screenshot",
    "rules": [
        {
            "description": "Eject to Screenshot",
            "manipulators": [
                {
                    "from": {
                        "consumer_key_code": "eject"
                    },
                    "to": [
                        {
                            "key_code": "5",
